{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "cache": {
    "cacheFormat": "universal",
    "cacheLocation": "../.cache/cspell-cache",
    "cacheStrategy": "content",
    "useCache": true
  },
  // https://github.com/streetsidesoftware/cspell-dicts
  "dictionaries": [
    "bash",
    "coding-compound-terms",
    "computing-acronyms",
    "css",
    "dotfiles",
    "en",
    "en-slang",
    "en-us",
    "filetypes",
    "fullstack",
    "git",
    "google",
    "html",
    "html-symbol-entities",
    "lorem",
    "networking-terms",
    "node",
    "npm",
    "programming",
    "project-words",
    "software-term-suggestions",
    "software-tools",
    "softwareTerms",
    "tridactyl",
    "typescript",
    "web-services"
  ],
  "dictionaryDefinitions": [
    {
      "addWords": true,
      "name": "dots",
      "path": "./cspell/dots.txt"
    },
    {
      "addWords": true,
      "name": "names",
      "path": "./cspell/names.txt"
    },
    {
      "addWords": true,
      "name": "tmux",
      "path": "./cspell/tmux.txt"
    },
    {
      "addWords": true,
      "name": "tridactyl",
      "path": "./cspell/tridactyl.txt"
    },
    {
      "addWords": true,
      "name": "zsh",
      "path": "./cspell/zsh.txt"
    },
    {
      "addWords": true,
      "name": "zsh-options",
      "path": "./cspell/zsh-options.txt"
    }
  ],
  "enableGlobDot": true,
  "ignorePaths": [
    ".*ignore",
    ".cache",
    ".git",
    ".github",
    ".prettierignore",
    ".vscode/{extensions,settings,launch}.json"
  ],
  "language": "en_US",
  "overrides": [
    {
      "filename": "*/tridactyl/*",
      "dictionaries": ["tridactyl"]
    },
    {
      "filename": "*/tmux/*",
      "dictionaries": ["tmux"]
    },
    {
      "filename": "*/zsh/*|*zsh*",
      "dictionaries": ["zsh", "zsh-options"]
    }
  ],
  "useGitignore": true,
  "version": "0.2"
}
