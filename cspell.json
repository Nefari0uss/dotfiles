{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "cache": {
    "cacheFormat": "universal",
    "cacheLocation": ".cache/cspell-cache",
    "cacheStrategy": "content",
    "useCache": true
  },
  "dictionaries": [
    "bash",
    "css",
    "en_US",
    "filetypes",
    "html",
    "node",
    "npm",
    "programming",
    "project-words",
    "softwareTerms",
    "typescript"
  ],
  "dictionaryDefinitions": [
    {
      "addWords": true,
      "name": "zsh-options",
      "path": ".vscode/zsh-options.txt"
    }
  ],
  "enableGlobDot": true,
  "ignorePaths": [
    ".git",
    ".vscode/{extensions,settings,launch}.json"
  ],
  "language": "en_US",
  "useGitignore": true,
  "words": [
    "gitstatus",
    "optname",
    "Powerline"
  ]
}
